{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.t1": {
      "initial_state": "default",
      "states": {
        "default": {
          "on_entry": [
            "/scoreboard players add @s m 1"
          ],
          "transitions": [
            {
              "loop": "math.mod(query.time_stamp,2) == 0"
            }
          ]
        },
        "loop": {
          "on_entry": [
            "/scoreboard players add @s m 1"
          ],
          "transitions": [
            {
              "default": "(1.0)"
            }
          ]
        }
      }
    }
  }
}