{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.yoni.guxi": {
      "initial_state": "init",
      "states": {
        "init": {
          "transitions": [
            {
              "loop1": "(1.0)"
            }
          ],
          "on_exit": [
            "/function yoni/init/guxi"
          ]
        },
        "loop1": {
          "on_entry": [
            "/function yoni/guxi/loop"
          ],
          "transitions": [
            {
              "loop2": "(1.0)"
            }
          ]
        },
        "loop2": {
          "on_entry": [
            "/function yoni/guxi/loop"
          ],
         "transitions": [
            {
              "loop1": "(1.0)"
            }
          ]
        }   
      }
    }
  }
}