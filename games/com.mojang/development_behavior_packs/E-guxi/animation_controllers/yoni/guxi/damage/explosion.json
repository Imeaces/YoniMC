{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.yoni.damage.explosion": {
      "initial_state": "defalut",
      "states": {
        "defalut": {
          "transitions": [
            {
              "explosion_damage": "query.is_sheared"
            }
          ]
        },
        "explosion_damage": {
          "on_entry": [
            "/function yoni/guxi/damage/explosion"
          ],
          "transitions": [
            {
              "defalut": "(1.0)"
            }
          ]
        }   
      }
    }
  }
}
