{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.minecraft.species.player": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [
            { "stage_loop": "query.scoreboard('species') == 42" },
            { "stage_init": "(1.0)" }
          ]
        },
        "stage_init": {
          "transitions": [
            { "init_being": "query.scoreboard('species') == 42" }
          ]
        },
        "stage_loop": {
          "transitions": [
            { "play_loop": "query.scoreboard('species') == 42" },
            { "play_perish": "(1.0)" }
          ],
          "on_entry": [ "/function yoni/guxi/event_reload" ]
        },
        "init_being": {
          "transitions": [
            { "play_being": "query.scoreboard('species') == 42" },
            { "stage_init": "(1.0)" }
          ]
        },
        "play_being": {
          "transitions": [
            { "play_loop": "query.scoreboard('species') == 42" },
            { "play_perish": "(1.0)" }
          ],
          "on_entry": [ "/function minecraft/player/species/push" ]
        },
        "play_perish": {
          "transitions": [
            { "stage_init": "(1.0)" }
          ],
          "on_entry": [ "/function minecraft/player/species/pop" ]
        },
        "play_loop": {
          "transitions": [
            { "play_loop1": "query.scoreboard('species') == 42" },
            { "play_perish": "(1.0)" }
          ]
        },
        "play_loop1": {
          "transitions": [
            { "play_loop": "query.scoreboard('species') == 42" },
            { "play_perish": "(1.0)" }
          ]
        }
      }
    }
  }
}